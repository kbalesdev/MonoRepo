{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "dispatch": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/dispatch",
      "sourceRoot": "projects/dispatch/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/dispatch",
            "index": "projects/dispatch/src/index.html",
            "main": "projects/dispatch/src/main.ts",
            "polyfills": "projects/dispatch/src/polyfills.ts",
            "tsConfig": "projects/dispatch/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/dispatch/src/favicon.ico",
              "projects/dispatch/src/assets"
            ],
            "styles": [
              "projects/dispatch/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/dispatch/src/environments/environment.ts",
                  "with": "projects/dispatch/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "dispatch:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "dispatch:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "dispatch:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/dispatch/src/test.ts",
            "polyfills": "projects/dispatch/src/polyfills.ts",
            "tsConfig": "projects/dispatch/tsconfig.spec.json",
            "karmaConfig": "projects/dispatch/karma.conf.js",
            "assets": [
              "projects/dispatch/src/favicon.ico",
              "projects/dispatch/src/assets"
            ],
            "styles": [
              "projects/dispatch/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/dispatch/tsconfig.app.json",
              "projects/dispatch/tsconfig.spec.json",
              "projects/dispatch/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/dispatch/e2e/protractor.conf.js",
            "devServerTarget": "dispatch:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "dispatch:serve:production"
            }
          }
        }
      }
    },
